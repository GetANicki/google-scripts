{
  "name": "google-forms-automation",
  "version": "1.0.0",
  "description": "## Local Development Setup",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GetANicki/google-forms-automation.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GetANicki/google-forms-automation/issues"
  },
  "homepage": "https://github.com/GetANicki/google-forms-automation#readme",
  "engines": {
    "node": ">= 12"
  },
  "scripts": {
    "build": "run-s clean build:webpack",
    "build:prod": "npm run clean && NODE_ENV=production webpack --mode=production",
    "build:webpack": "webpack --mode=none",
    "clean": "rimraf dist",
    "compile": "tsc -p . --noEmit",
    "deploy:dev": "run-s build deploy:NickiAdmin",
    "deploy:prod": "run-s build:prod update-prod-project-id deploy:NickiAdmin",
    "deploy:NickiAdmin": "cd dist/NickiAdmin && clasp push -f",
    "update-prod-project-id": "sed -i '' -e 's|1972_r7VW7xV1mkfWQh3GbTGjsgaduvzKOl0Z3-u4gkoWlHtQHGifhyj-|1bzVP1cnimW2fOz7Xo_j0165xqTFyZsP31puT0zIsi0lWkScqBflRUeQo|' dist/NickiAdmin/.clasp.json",
    "test": "echo \"No tests.  Success...?\"",
    "fix-broken-types": "cd node_modules/@types/google-apps-script/ && sed -i.bak -e 's|declare var console: Google|//|' google-apps-script.base.d.ts && rm google-apps-script.base.d.ts.bak",
    "prepare": "husky install",
    "postinstall": "npm run fix-broken-types"
  },
  "devDependencies": {
    "@types/google-apps-script": "^1.0.45",
    "@types/jest": "^27.4.1",
    "@types/json2csv": "^5.0.3",
    "@types/node": "^17.0.22",
    "clasp": "^1.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "dotenv-webpack": "^7.1.0",
    "foreach-folder": "^1.1.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "json2csv": "^5.0.7",
    "lint-staged": "^12.3.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.8",
    "type-fest": "^2.12.1",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "lint-staged": {
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "camelcase": "^6.3.0"
  }
}
